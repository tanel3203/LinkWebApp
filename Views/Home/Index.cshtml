@{
    ViewData["Title"] = "Koduleht";
}
@using Link




<br><br><br> 


<form action="Home/Submitted" method="POST">

        <h1> Isiklik info </h1>
        <div class="row">
            <div class="col-md-12">
              <div class="form-group">
                <label for="inputFirstName">Eesnimi</label>
                <input type="text" class="form-control" name="inputFirstName" placeholder="Jane">
              </div>
              <div class="form-group">
                <label for="inputLastName">Perenimi</label>
                <input type="text" class="form-control" name="inputLastName" placeholder="Doe">
              </div>
              <div class="form-group">
                <label for="inputBirthDate">S체nnikuup채ev</label>
                <input type="date" class="form-control" name="inputBirthDate">
              </div>
            </div>
        </div>


        
        <h1> Muu info </h1>
        <div class="row">
            <div class="col-md-4">
              <div class="form-group">
                <label for="inputTitle">Pealkiri</label>
                <input type="text" class="form-control" name="inputTitle" placeholder="Legend of Zelda">
              </div>
            </div>
            <div class="col-md-4">
              <div class="form-group">
                <label for="inputUrl">URL</label>
                <input type="url" class="form-control" name="inputUrl" placeholder="must start with http://">
              </div>
            </div>
        </div> 
        <div class="row">        
            <div class="col-md-4">
              <div class="form-group">
                <label for="inputDescription">Kirjeldus</label>
                <input type="textarea" class="form-control" name="inputDescription" placeholder="Zelda does ...">
              </div>
            </div>
            <div class="col-md-4">
              <div class="form-group">
                <label for="inputOwnerName">Omaniku nimi</label>
                <input type="text" class="form-control" name="inputOwnerName" placeholder="John Doe">
              </div>
            </div> 
        </div> 
          <div class="row">
            <div class="col-md-4">
              <div class="form-group">
                <label for="inputCategory">Kategooria</label>
                <select name="inputCategory">
                  <option value="meelelahutus">meelelahutus</option>
                  <option value="tehnoloogiauudised">tehnoloogiauudised</option>
                  <option value="muu">muu</option>
                </select>
              </div>
            </div>
            <div class="col-md-4">
              <div class="form-group">
                <label for="inputPoints">Punktid</label>
                <select name="inputPoints">
                  <option value="1">1</option>
                  <option value="2">2</option>
                  <option value="3">3</option>
                  <option value="4">4</option>
                  <option value="5">5</option>
                  <option value="6">6</option>
                  <option value="7">7</option>
                  <option value="8">8</option>
                  <option value="9">9</option>
                  <option value="10">10</option>
                </select>
              </div>
            </div>
        </div> 


 <input type="submit" value="View Report">
</form>








